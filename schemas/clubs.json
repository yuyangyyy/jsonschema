{
"$schema": "https://json-schema.org/draft-07/schema",
"$id": "https://example.com/clubs.sechema.json",
"title": "clubs",
"description":"creer une table clubs",
"type":"object",

"properties":{
    "id":{
        "type": "number"
    },
    "name":{
        "type": "string"
    },

    "address":{
        "street_type":{
            "type":"string",
            "enum":["Street","Avenue","Boulevard"]
        },
        "city":{
            "type":"string"
        },
        "state":{
            "type":"string"
        }
    },
        "phoneNumber":{
            "type": "number"
        },
        "email":{
             "type":"string",
             "format": "email"
        } 
},

"required":["id","name","address","phoneNumber","email"]

}