{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://example.com/players.schema.json",
    "title": "players",
    "description": "create a players of clubs",
    "type": "object",
    "$ref": "https://example.com/members.schema.json",
    "properties": {
        "age": {
            "type": "number",
            "minimum": 0
        },
        "hobbies": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "email": {
            "type": "string"
        },
        "members_id": {
            "type": "string"
        }
    },
    "required": [
        "age",
        "members_id",
        "email",
        "hobbies"
    ]
}